<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/shape_color_tag">
        <shape
            android:shape="rectangle">
            <solid
                android:color="@color/colorHighlight"/>
            <corners
